<!DOCTYPE html>
<html>
{% include "partials/site/head.html.twig" %}
<body>
    <div class="wrap">
        {% include 'partials/site/header.html.twig' %}

        <div class="content">
            {% include "partials/site/flashmessage.html.twig" %}

            {% block contentabove %}{% endblock %}

            <div class="navigation top">
                {% block findnavigation %}{% endblock %}
            </div>

            <div class="results">
                <section class="facets">
                    {% block sidebar %}{% endblock %}
                </section>
                {% block content %}{% endblock %}
            </div>
        </div>

        {% include 'partials/site/footer.html.twig' %}
        {% include 'partials/site/colophon.html.twig' %}
    </div>

    {% block javascripts %}
        <script src="{{ asset('js/vendor/jquery.min.js') }}"></script>
        <script src="{{ asset('js/vendor/jquery.lazyload.js') }}"></script>
        <script src="{{ asset('js/vendor/jquery.mousewheel.js') }}"></script>
        <script src="{{ asset('js/vendor/jquery.panzoom.min.js') }}"></script>

        <script src="{{ asset('js/sub-legacy.js') }}"></script>
        <script src="{{ asset('js/script.js') }}"></script>
    {% endblock %}
</body>
</html>
