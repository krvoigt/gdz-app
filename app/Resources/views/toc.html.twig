<ul>
{% for element in structure %}
    <li><a href="{{ path('subugoe_find_detail', {'id': id ~ '|' ~ element.id}) }}">{{ element.label }}</a>

        {% if element.children is defined %}
            <ul>
                {% for children in element.children %}
                    <li>{{ element.children }}</li>
                {% endfor %}
            </ul>
        {% endif %}

    </li>
{% endfor %}
</ul>
