<!DOCTYPE html>
<html>
{% include "partials/site/head.html.twig" %}
<body>
    <div class="wrap">
        {% include 'partials/site/header.html.twig' %}

        <div class="content">
            <ul>
            {% for element in structure %}
                <li><a href="{{ path('subugoe_find_detail', {'id': id ~ '|' ~ element.id}) }}">{{ element.label }}</a>

                    {% if element.children is defined %}
                        <ul>
                            {% for children in element.children %}
                                <li>{{ element.children }}</li>
                            {% endfor %}
                        </ul>
                    {% endif %}

                </li>
            {% endfor %}
            </ul>
        </div>

        {% include 'partials/site/footer.html.twig' %}

        {% include 'partials/site/colophon.html.twig' %}

    </div>
</body>
</html>
